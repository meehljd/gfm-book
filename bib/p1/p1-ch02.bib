}@article{gusev_twas_2016,
	title = {Integrative approaches for large-scale transcriptome-wide association studies},
	volume = {48},
	copyright = {2016 Springer Nature America, Inc.},
	issn = {1546-1718},
	url = {https://www.nature.com/articles/ng.3506},
	doi = {10.1038/ng.3506},
	abstract = {Alexander Gusev, Bogdan Pasaniuc and colleagues present a strategy that integrates gene expression measurements with summary statistics from large-scale genome-wide association studies to identify genes whose cis-regulated expression is associated with complex traits. They identify 69 new genes significantly associated with obesity-related traits and illustrate how this approach can provide insights into the genetic basis of complex traits.},
	language = {en},
	number = {3},
