	file = {Full Text PDF:/Users/meehl.joshua/Zotero/storage/LP7T6PUJ/2015-03-01 - Quang et al. - DANN a deep learning approach for annotating the pathogenicity of genetic variants.pdf:application/pdf;Snapshot:/Users/meehl.joshua/Zotero/storage/9J2B5MFN/btu703.html:text/html},
}

@article{quang_danq_2016,
