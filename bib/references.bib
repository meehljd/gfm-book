@misc{elnaggar_prottrans_2021,
	title = {{ProtTrans}: {Towards} {Cracking} the {Language} of {Life}'s {Code} {Through} {Self}-{Supervised} {Deep} {Learning} and {High} {Performance} {Computing}},
